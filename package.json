{
  "name": "flybook",
  "version": "1.4.1",
  "main": "./dist/app.js",
  "license": "MIT",
  "homepage": "https://rhiokim.github.io/flybook",
  "repository": {
    "url": "https://github.com/rhiokim/flybook",
    "type": "git"
  },
  "author": {
    "name": "Rhio Kim",
    "email": "rhio.kim@gmail.com",
    "url": "https://flybook.github.io/"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "fly",
    "release": "fly release",
    "serve": "serve out -s -p 5002",
    "docs": "./dist/bin/cli.js docs --outdir=out --font=Overpass --codeStyle=tomorrow-night",
    "docs:prod": "./dist/bin/cli.js docs --outdir=out --prod --font=Overpass --codeStyle=solarized-light --theme=light",
    "pages": "npm run docs:prod && gh-pages -d out",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty 'libs/**/*.@(js|jsx)'",
    "lint:msg": "validate-commit-msg",
    "format": "prettier-eslint --write 'libs/**/*.@(js|jsx)'",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report",
    "cz": "git cz",
    "precommit": "flow && lint-staged",
    "commitmsg": "npm run lint:msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "babel-register": "^6.24.1",
    "classnames": "^2.2.5",
    "deep-assign": "^2.0.0",
    "del": "^3.0.0",
    "eslint-plugin-node": "^5.0.0",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "inquirer": "^3.1.0",
    "js-yaml": "^3.8.4",
    "json2yaml": "^1.1.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "recursive-copy": "^2.0.6",
    "remark": "^7.0.1",
    "remark-autolink-headings": "^5.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-html": "^6.0.0",
    "remark-slug": "^4.2.2",
    "titleize": "^1.0.0",
    "update-notifier": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.0.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.48.0",
    "flow-coverage-report": "^0.3.0",
    "fly": "^2.0.6",
    "fly-babel": "^2.1.1",
    "fly-clear": "^1.0.1",
    "fly-esnext": "^2.0.1",
    "fly-watch": "^1.1.1",
    "gh-pages": "^1.0.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "node-notifier": "^5.1.2",
    "prettier-eslint": "^6.3.0",
    "prettier-eslint-cli": "^4.1.1",
    "replace": "^0.3.0",
    "validate-commit-msg": "^2.12.2"
  },
  "bin": {
    "flybook": "./dist/bin/cli.js"
  },
  "files": [
    "dist",
    "static",
    "index.js"
  ],
  "flow-coverage-report": {
    "includeGlob": [
      "libs/**/*.js",
      "components/**/*.js",
      "pages/**/*.js"
    ],
    "type": [
      "text"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "preset": "angular",
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "libs/**/*.{js,jsx}": [
      "format",
      "lint",
      "git add"
    ],
    "components/**/*.{js,jsx}": [
      "format",
      "lint",
      "git add"
    ],
    "pages/**/*.{js,jsx}": [
      "format",
      "lint",
      "git add"
    ]
  },
  "contributors": [{
    "name": "Uno Kim",
    "email": "djkehh@gmail.com",
    "url": "https://djkeh.github.io/"
  }]
}
